{
  "name": "GARDNR - GPT-3 Sales Assistant",
  "description": "Write your pitch email for Microsoft Azure Products targeted for a particular customer",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "48": "assets/48.png",
    "72": "assets/72.png",
    "96": "assets/96.png",
    "144": "assets/144.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Understand Sales Strategy"
  },
  "background": {
    "service_worker": "scripts/contextMenuServiceWorker.js"
  },

  "permissions": ["contextMenus", "tabs", "storage"]
}
